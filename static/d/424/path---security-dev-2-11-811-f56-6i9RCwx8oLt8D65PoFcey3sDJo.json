ginx.conf        …        server {            #listen       80 default_server;            #listen       [::]:80 default_server;            listen  443     ssl;            server_name  localhost;            ssl_certificate /home/ec2-user/secure/cert.pem;            ssl_certificate_key     /home/ec2-user/secure/key.pem;            ssl_protocols   TLSv1 TLSv1.1 TLSv1.2;            ssl_ciphers         HIGH:!aNULL:!MD5;            root         /usr/share/nginx/html;    …    }       ","html":"\n\t\t<div style=\"position:absolute;left:48px;top:163px;width:576px\">\n\t\t\t<h2 lang=\"en-US\" style=\"font-size:14pt;color:#2e75b5;margin-top:0pt;margin-bottom:0pt\">&#xACBD;&#xB85C; : /etc/nginx/nginx.conf</h2>\n\t\t\t<br>\n\t\t\t<p lang=\"en-US\" style=\"margin-top:0pt;margin-bottom:0pt\">&#x2026;</p>\n\t\t\t<p lang=\"en-US\" style=\"margin-top:0pt;margin-bottom:0pt\">    server {</p>\n\t\t\t<p lang=\"en-US\" style=\"margin-top:0pt;margin-bottom:0pt\">        #listen       80 default_server;</p>\n\t\t\t<p lang=\"en-US\" style=\"margin-top:0pt;margin-bottom:0pt\">        #listen       [::]:80 default_server;</p>\n\t\t\t<p lang=\"en-US\" style=\"margin-top:0pt;margin-bottom:0pt\">        <span style=\"font-weight:bold\">listen  443     ssl;</span></p>\n\t\t\t<p lang=\"en-US\" style=\"margin-top:0pt;margin-bottom:0pt\"><span style=\"font-weight:bold\">        server_name  localhost;</span></p>\n\t\t\t<p lang=\"en-US\" style=\"margin-top:0pt;margin-bottom:0pt\"><span style=\"font-weight:bold\">        ssl_certificate /home/ec2-user/secure/cert.pem;</span></p>\n\t\t\t<p lang=\"en-US\" style=\"margin-top:0pt;margin-bottom:0pt\"><span style=\"font-weight:bold\">        ssl_certificate_key     /home/ec2-user/secure/key.pem;</span></p>\n\t\t\t<p lang=\"en-US\" style=\"margin-top:0pt;margin-bottom:0pt\"><span style=\"font-weight:bold\">        ssl_protocols   TLSv1 TLSv1.1 TLSv1.2;</span></p>\n\t\t\t<p lang=\"en-US\" style=\"margin-top:0pt;margin-bottom:0pt\"><span style=\"font-weight:bold\">        ssl_ciphers         HIGH:!aNULL:!MD5;</span></p>\n\t\t\t<p lang=\"en-US\" style=\"margin-top:0pt;margin-bottom:0pt\">        root         /usr/share/nginx/html;</p>\n\t\t\t<p lang=\"en-US\" style=\"margin-top:0pt;margin-bottom:0pt\">&#x2026;</p>\n\t\t\t<p lang=\"en-US\" style=\"margin-top:0pt;margin-bottom:0pt\">}</p>\n\t\t</div>\n\t\n\n"}}}