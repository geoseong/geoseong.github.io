for a in enumerate(list):        print('Case1(a[0],a[1]) {}: {}'.format(a[0], a[1]))        # Case 2    for a in enumerate(list):        print('Case2(*a) {}: {}'.format(*a))        Case1(a[0],a[1]) 0: 1    Case1(a[0],a[1]) 1: 2    Case1(a[0],a[1]) 2: 3    Case1(a[0],a[1]) 3: 4    Case1(a[0],a[1]) 4: 5    Case2(*a) 0: 1    Case2(*a) 1: 2    Case2(*a) 2: 3    Case2(*a) 3: 4    Case2(*a) 4: 5           ","html":"\n\t\t<div style=\"position:absolute;left:39px;top:123px;width:584px\">\n\t\t\t<h2 lang=\"en-US\" style=\"font-size:14pt;color:#2e75b5;margin-top:0pt;margin-bottom:0pt\">enumerate<span lang=\"ko-KR\">&#xB97C;</span> <span lang=\"ko-KR\">&#xC774;&#xC6A9;&#xD55C;</span> <span lang=\"ko-KR\">&#xD29C;&#xD50C;&#xC744;</span> <span lang=\"ko-KR\">&#xC774;&#xC6A9;&#xD55C;</span> <span lang=\"ko-KR\">&#xBC18;&#xBCF5;&#xBB38;&#xCC98;&#xB9AC;</span></h2>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">list = [1,2,3,4,5]</p>\n\t\t\t<br>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\"># Case 1</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">for a in enumerate(list):</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">    print(&apos;Case1(a[0],a[1]) {}: {}&apos;.format(a[0], a[1]))</p>\n\t\t\t<br>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\"># Case 2</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">for a in enumerate(list):</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">    print(&apos;Case2(*a) {}: {}&apos;.format(*a))</p>\n\t\t\t<br>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">Case1(a[0],a[1]) 0: 1</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">Case1(a[0],a[1]) 1: 2</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">Case1(a[0],a[1]) 2: 3</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">Case1(a[0],a[1]) 3: 4</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">Case1(a[0],a[1]) 4: 5</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">Case2(*a) 0: 1</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">Case2(*a) 1: 2</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">Case2(*a) 2: 3</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">Case2(*a) 3: 4</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">Case2(*a) 4: 5</p>\n\t\t\t<br>\n\t\t</div>\n\t\n\n"}}}