       console.log('[files]',files);                    });                // 텍스트파일 존재여부 체크.        fs.exists(dir+filename, function (exists) {             var data = '\\tfile system example!!\\n';            console.log('exists?', exists);            exists ? append(data) : write(data);        });            // 없는 파일 새로 쓰기        function write(data){            var options = {encoding:'utf8', flag:'w'};            return fs.writeFile(dir+filename, data, options, function(error){                 console.log('path', dir+filename);            });        }            // 파일 이어서 쓰기        function append(data){            return fs.appendFile(dir+filename, data, function (err) {                 if (err) throw err;                 console.log('The '+data+' was appended to file!');             });        }                // stat() : 정보확인         fs.stat(dir+'/'+filename, function(err, stats) {             console.log('[stats]',stats);         });            res.send(data);       ","html":"\n\t\t<div style=\"position:absolute;left:48px;top:139px;width:576px\">\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">    var filename = &apos;nursestation_log.txt&apos;;</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">    var dir = &apos;./log/&apos;;</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">    </p>\n\t\t\t<h2 style=\"font-family:Calibri;font-size:14pt;color:#2e75b5;margin-top:0pt;margin-bottom:0pt\">    // <span style=\"font-family:Malgun Gothic\">&#xB514;&#xB809;&#xD1A0;&#xB9AC;</span> <span style=\"font-family:Malgun Gothic\">&#xCCB4;&#xD06C;</span></h2>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">    fs.readdir(dir, function (err, files) {</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">        if(err){</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">            fs.mkdir(dir, function(err) {</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">              if(err) throw err;</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">            });</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">        }</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">        console.log(&apos;[files]&apos;,files);</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">        </p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">    });</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">    </p>\n\t\t\t<h2 style=\"font-family:Calibri;font-size:14pt;color:#2e75b5;margin-top:0pt;margin-bottom:0pt\">    // <span style=\"font-family:Malgun Gothic\">&#xD14D;&#xC2A4;&#xD2B8;&#xD30C;&#xC77C;</span> <span style=\"font-family:Malgun Gothic\">&#xC874;&#xC7AC;&#xC5EC;&#xBD80;</span> <span style=\"font-family:Malgun Gothic\">&#xCCB4;&#xD06C;</span>.</h2>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">    fs.exists(dir+filename, function (exists) { </p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">        var data = &apos;\\tfile system example!!\\n&apos;;</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">        console.log(&apos;exists?&apos;, exists);</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">        exists ? append(data) : write(data);</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">    });</p>\n\t\t\t<br>\n\t\t\t<h2 style=\"font-family:Calibri;font-size:14pt;color:#2e75b5;margin-top:0pt;margin-bottom:0pt\">    // <span style=\"font-family:Malgun Gothic\">&#xC5C6;&#xB294;</span> <span style=\"font-family:Malgun Gothic\">&#xD30C;&#xC77C;</span> <span style=\"font-family:Malgun Gothic\">&#xC0C8;&#xB85C;</span> <span style=\"font-family:Malgun Gothic\">&#xC4F0;&#xAE30;</span></h2>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">    function write(data){</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">        var options = {encoding:&apos;utf8&apos;, flag:&apos;w&apos;};</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">        return fs.writeFile(dir+filename, data, options, function(error){ </p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">            console.log(&apos;path&apos;, dir+filename);</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">        });</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">    }</p>\n\t\t\t<br>\n\t\t\t<h2 style=\"font-family:Calibri;font-size:14pt;color:#2e75b5;margin-top:0pt;margin-bottom:0pt\">    // <span style=\"font-family:Malgun Gothic\">&#xD30C;&#xC77C;</span> <span style=\"font-family:Malgun Gothic\">&#xC774;&#xC5B4;&#xC11C;</span> <span style=\"font-family:Malgun Gothic\">&#xC4F0;&#xAE30;</span></h2>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">    function append(data){</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">        return fs.appendFile(dir+filename, data, function (err) { </p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">            if (err) throw err; </p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">            console.log(&apos;The &apos;+data+&apos; was appended to file!&apos;); </p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">        });</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">    }</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">    </p>\n\t\t\t<h2 style=\"font-family:Calibri;font-size:14pt;color:#2e75b5;margin-top:0pt;margin-bottom:0pt\">    // stat() : <span style=\"font-family:Malgun Gothic\">&#xC815;&#xBCF4;&#xD655;&#xC778;</span></h2>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">     fs.stat(dir+&apos;/&apos;+filename, function(err, stats) {</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">         console.log(&apos;[stats]&apos;,stats);</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">     });</p>\n\t\t\t<br>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">    res.send(data);</p>\n\t\t</div>\n\t\n\n"}}}