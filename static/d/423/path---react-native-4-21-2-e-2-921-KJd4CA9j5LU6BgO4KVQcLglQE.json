Platform, StyleSheet} from 'react-native';     const styles = StyleSheet.create({     height: Platform.OS === 'ios' ? 200 : 100,     });        For example, say you have the following files in your project:    BigButton.ios.js￼BigButton.android.js￼    You can then require the component as follows:    const BigButton = require('./BigButton');￼           ","html":"\n\t\t<div style=\"position:absolute;left:48px;top:115px;width:576px\">\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\"><a href=\"https://facebook.github.io/react-native/docs/platform-specific-code.html\">https://facebook.github.io/react-native/docs/platform-specific-code.html</a></p>\n\t\t\t<br>\n\t\t\t<p lang=\"en-US\" style=\"margin-top:0pt;margin-bottom:0pt\">Example</p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\"><span style=\"font-size:10pt;color:#859900\">import</span><span style=\"font-size:10pt\"> {</span><span style=\"font-size:10pt;font-weight:bold\">Platform</span><span style=\"font-size:10pt\">, StyleSheet} </span><span style=\"font-size:10pt;color:#859900\">from </span><span style=\"font-size:10pt;color:#2aa198\">&apos;react-native&apos;</span><span style=\"font-size:10pt\">; </span></p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\"><span style=\"font-size:10pt;color:#859900\">const</span><span style=\"font-size:10pt\"> styles = StyleSheet.create({ </span></p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\"><span style=\"font-size:10pt;color:#b58900\">height</span><span style=\"font-size:10pt\">: Platform.OS === </span><span style=\"font-size:10pt;color:#2aa198\">&apos;ios&apos;</span><span style=\"font-size:10pt\"> ? </span><span style=\"font-size:10pt;color:#2aa198\">200</span><span style=\"font-size:10pt\"> : </span><span style=\"font-size:10pt;color:#2aa198\">100</span><span style=\"font-size:10pt\">, </span></p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\"><span style=\"font-size:10pt\">});</span></p>\n\t\t\t<br>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\"><span style=\"font-size:10pt;color:#393939;background-color:#f5fcff\">For example, say you have the following files in your project:</span></p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\"><span style=\"font-size:10pt;color:#839496;background-color:#f5fcff\">BigButton.ios.js</span><span style=\"font-size:10pt;color:#839496;background-color:#f5fcff\">&#xFFFC;</span><span style=\"font-size:10pt;color:#839496;background-color:#f5fcff\">BigButton.android.js</span><span style=\"font-size:10pt;color:#839496;background-color:#f5fcff\">&#xFFFC;</span></p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\"><span style=\"font-size:10pt;color:#393939;background-color:#f5fcff\">You can then require the component as follows:</span></p>\n\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\"><span style=\"font-size:10pt;color:#859900;background-color:#f5fcff\">const</span><span style=\"font-size:10pt;color:#839496;background-color:#f5fcff\"> BigButton = </span><span style=\"font-size:10pt;color:#dc322f;background-color:#f5fcff\">require</span><span style=\"font-size:10pt;color:#839496;background-color:#f5fcff\">(</span><span style=\"font-size:10pt;color:#2aa198;background-color:#f5fcff\">&apos;./BigButton&apos;</span><span style=\"font-size:10pt;color:#839496;background-color:#f5fcff\">);</span><span style=\"font-size:10pt;color:#839496;background-color:#f5fcff\">&#xFFFC;</span></p>\n\t\t\t<br>\n\t\t</div>\n\t\n\n"}}}