'express');      var router = express.Router();      require('dotenv').config();            router.get('/dotenv', function(req, res, next) {          // DB_NAME 출력          res.send(process.env.DB_NAME);      });            module.exports = router;                      ","html":"\n\t\t<div style=\"position:absolute;left:48px;top:139px;width:576px\">\n\t\t\t<h2 lang=\"ko-KR\" style=\"font-size:14pt;color:#2e75b5;margin-top:0pt;margin-bottom:0pt\">dotenv <span style=\"font-family:Malgun Gothic\">&#xD328;&#xD0A4;&#xC9C0;</span><span lang=\"en-US\" style=\"font-family:Malgun Gothic\"> </span><span style=\"font-family:Malgun Gothic\">&#xC0AC;&#xC6A9;</span></h2>\n\t\t\t<p lang=\"ko-KR\" style=\"margin-top:0pt;margin-bottom:0pt\">Node.js <span style=\"font-family:Malgun Gothic\">&#xC5D0;&#xC11C;</span><span lang=\"en-US\"> </span>.env<span style=\"font-family:Malgun Gothic\">&#xB97C;</span> <span style=\"font-family:Malgun Gothic\">&#xC0AC;&#xC6A9;&#xD558;&#xB824;&#xBA74;</span><span lang=\"en-US\"> dotenv </span><span style=\"font-family:Malgun Gothic\">&#xD328;&#xD0A4;&#xC9C0;&#xB97C;</span><span lang=\"en-US\" style=\"font-family:Malgun Gothic\"> </span><span style=\"font-family:Malgun Gothic\">&#xC124;&#xCE58;&#xD55C;&#xB2E4;</span><span lang=\"en-US\" style=\"font-family:Malgun Gothic\">.</span></p>\n\t\t\t<table style=\"border:0px\">\n\t\t\t\t<tbody><tr>\n\t\t\t\t\t<td style=\"background-color:#f2f2f2;border:0px\">$ <span style=\"font-family:Malgun Gothic\">npm install dotenv --save</span></td>\n\t\t\t\t</tr>\n\t\t\t</tbody></table>\n\t\t\t<br>\n\t\t\t<h3 style=\"color:#5b9bd5;margin-top:0pt;margin-bottom:0pt\">Index.js</h3>\n\t\t\t<table style=\"border:0px\">\n\t\t\t\t<tbody><tr>\n\t\t\t\t\t<td style=\"background-color:#f2f2f2;border:0px\"><p style=\"margin-top:0pt;margin-bottom:0pt\"><span style=\"color:#70ad47\">// routes.index.js</span></p>\n\t\t\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\"><span style=\"color:#70ad47\">// </span><span style=\"font-family:Malgun Gothic;color:#70ad47\">&#xD574;&#xB2F9;</span><span style=\"color:#70ad47\"> </span><span style=\"font-family:Malgun Gothic;color:#70ad47\">&#xD504;&#xB85C;&#xC81D;&#xD2B8;&#xB294;</span><span style=\"color:#70ad47\"> express-generator</span><span style=\"font-family:Malgun Gothic;color:#70ad47\">&#xB97C;</span><span style=\"color:#70ad47\"> </span><span style=\"font-family:Malgun Gothic;color:#70ad47\">&#xC0AC;&#xC6A9;&#xD558;&#xC5EC;</span><span style=\"color:#70ad47\"> </span><span style=\"font-family:Malgun Gothic;color:#70ad47\">&#xC0DD;&#xC131;&#xD568;</span></p>\n\t\t\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">var express = require(&apos;express&apos;);</p>\n\t\t\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">var router = express.Router();</p>\n\t\t\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\"><span style=\"font-size:14pt;color:#0432ff;font-weight:bold;text-decoration:underline\">require(&apos;dotenv&apos;).config();</span></p>\n\t\t\t\t\t<br>\n\t\t\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">router.get(&apos;/dotenv&apos;, function(req, res, next) {</p>\n\t\t\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\"><span style=\"color:#70ad47\">    // DB_NAME </span><span style=\"font-family:Malgun Gothic;color:#70ad47\">&#xCD9C;&#xB825;</span></p>\n\t\t\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">    res.send(<span style=\"font-size:14pt;color:#0432ff;font-weight:bold\">process.env.DB_NAME</span>);</p>\n\t\t\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">});</p>\n\t\t\t\t\t<br>\n\t\t\t\t\t<p style=\"margin-top:0pt;margin-bottom:0pt\">module.exports = router;</p>\n\t\t\t\t\t</td>\n\t\t\t\t</tr>\n\t\t\t</tbody></table>\n\t\t</div>\n\t\n\n"}}}